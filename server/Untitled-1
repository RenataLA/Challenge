CREATE TABLE CANDIDATO(
cod_candidato int primary key identity(1,1),
nome varchar(30),
sobrenome varchar(30),
endereco varchar(50)
);

CREATE TABLE BIGFIVE(
cod_candidato int,
abertura decimal,
escrupulosidade decimal,
extroversao decimal,
amabilidade decimal,
faixa_emocional decimal
);



INSERT INTO CANDIDATO (nome, sobrenome, endereco) 
VALUES ('Rafael', 'Maia', 'Rua');

INSERT INTO BIGFIVE (cod_candidato, abertura, escrupulosidade, extroversao, amabilidade, faixa_emocional)
VALUES (1, 1.2, 1.3, 1.4, 1.6, 1.1)
